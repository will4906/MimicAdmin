<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <environments default="development">
        <environment id="development">
            <transactionManager type="JDBC"/>
            <dataSource type="POOLED">
                <property name="driver" value="org.postgresql.Driver"/>
                <property name="url" value="jdbc:postgresql://localhost:5432/tmimic?useAffectedRows=true"/>
                <property name="username" value="postgres"/>
                <property name="password" value="1234"/>
            </dataSource>
        </environment>
    </environments>
    <mappers>
        <mapper resource="mapper/ProjectMapper.xml"/>
        <mapper resource="mapper/HadmMapper.xml"/>
        <mapper resource="mapper/SubjectMapper.xml"/>
        <mapper resource="mapper/IcustayMapper.xml"/>
    </mappers>
</configuration>